{
  "name": "@datkt/konanc-config",
  "version": "3.2.1",
  "description": "Command line utility to read '*.kc' konanc config files to print compiler flags",
  "main": "index.js",
  "bin": {
    "konanc-config": "bin/konanc-config"
  },
  "scripts": {
    "test": "make test",
    "build": "pkg package.json -o build/konanc-config",
    "changelog": "conventional-changelog --same-file --preset angular --release-count 0 --infile CHANGELOG.md",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "keywords": [
    "konanc",
    "config",
    "kotlin",
    "native",
    "package",
    "config",
    "compiler",
    "flags"
  ],
  "author": "Joseph Werle <werle@littlstar.com>",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.1.0",
    "find-up": "^3.0.0",
    "minimist": "^1.2.0",
    "rc": "^1.2.8"
  },
  "devDependencies": {
    "conventional-changelog": "^3.0.5",
    "pkg": "^4.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datkt/konanc-config.git"
  },
  "bugs": {
    "url": "https://github.com/datkt/konanc-config/issues"
  },
  "homepage": "https://github.com/datkt/konanc-config#readme",
  "pkg": {
    "targets": [
      "node10-linux-x64",
      "node10-macos-x64",
      "node10-win-x64"
    ]
  }
}
